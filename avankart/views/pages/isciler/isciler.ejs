<script>
// const CSRF = document.querySelector("meta[name='csrf-token']")?.content || "";

/* Yalnız AP- */
function isValidPeopleId(v) {
  return typeof v === "string" && /^AP-/.test(v.trim()) && v.trim().length >= 5;
}

/* fullname → {name, surname} (ehtiyat üçün) */
function splitName(fullname) {
  if (!fullname || typeof fullname !== "string") return { name: "-", surname: "-" };
  const parts = fullname.trim().split(/\s+/);
  if (parts.length === 1) return { name: parts[0], surname: "-" };
  return { name: parts[0], surname: parts.slice(1).join(" ") };
}

/* gender EN → AZ vizual mətn */
function formatGender(g) {
  if (g === "male") return "Kişi";
  if (g === "female") return "Qadın";
  if (g === "other") return "Digər";
  return g || "-";
}

async function showIshciId() {
  const idInput  = document.getElementById("idInput");
  const errorEl  = document.getElementById("searchError");
  const userBox  = document.getElementById("userInfoResult");
  const nextPop  = document.getElementById("ishciId");
  const firstPop = document.getElementById("yeni-isci-div");

  errorEl?.classList.add("hidden");



  const peopleId = idInput?.value?.trim();
  if (!isValidPeopleId(peopleId)) {
    errorEl.textContent = "Düzgün People ID daxil et (məs: AP-XXXXXXXXXX)";
    errorEl.classList.remove("hidden");
    return;
  }


  try {
    const res = await fetch(`/people/user/${encodeURIComponent(peopleId)}`, {
      method: "POST",
          credentials: "include",   
      headers: {
        "Content-Type": "application/json",
        "CSRF-Token": document.querySelector("meta[name='csrf-token']").content
      },
      body: JSON.stringify({ partnyor_id: peopleId }), // <-- DÜZGÜN BODY AÇARI
    });

    if (!res.ok) {
      errorEl.textContent = "İstifadəçi tapılmadı";
      errorEl.classList.remove("hidden");
      return;
    }

    // Cavab array da ola bilər, obyekt də
    let payload;
    try { payload = await res.json(); } catch { payload = null; }
    const u = Array.isArray(payload) ? payload[0] : payload;

    if (!u || typeof u !== "object") {
      errorEl.textContent = "İstifadəçi tapılmadı";
      errorEl.classList.remove("hidden");
      return;
    }

    // Model: PeopleUser (name, surname, email, phone_suffix, phone, gender, people_id ...)
    const name       = u.name ?? splitName(u.fullname).name;
    const surname    = u.surname ?? splitName(u.fullname).surname;
    const genderText = formatGender(u.gender);
    const email      = u.email ?? "-";
    const phoneText  = (u.phone_suffix && u.phone) ? `+${u.phone_suffix} ${u.phone}` : (u.phoneNumber ?? u.phone ?? "-");
    const userId     = u._id ?? u.id ?? "";

    userBox.innerHTML = `
      <div class="flex justify-between border-b dark:border-[#FFFFFF1A] pb-3">
        <span class="opacity-65">Adı:</span><span class="font-medium">${name}</span>
      </div>
      <div class="flex justify-between border-b dark:border-[#FFFFFF1A] pb-3">
        <span class="opacity-65">Soyad:</span><span class="font-medium">${surname}</span>
      </div>
      <div class="flex justify-between border-b dark:border-[#FFFFFF1A] pb-3">
        <span class="opacity-65">Cinsi:</span><span class="font-medium">${genderText}</span>
      </div>
      <div class="flex justify-between border-b dark:border-[#FFFFFF1A] pb-3">
        <span class="opacity-65">Email:</span><span class="font-medium">${email}</span>
      </div>
      <div class="flex justify-between border-b dark:border-[#FFFFFF1A] pb-3">
        <span class="opacity-65">Telefon:</span><span class="font-medium">${phoneText}</span>
      </div>
      <input type="hidden" id="hiddenUserId" value="${userId}" />
    `;

    firstPop?.classList.add("hidden");
    nextPop?.classList.remove("hidden");
  } catch (e) {
    console.error("fetch error:", e);
    errorEl.textContent = "Şəbəkə xətası";
    errorEl.classList.remove("hidden");
  }
}

/* Click + Enter */
document.getElementById("submitByIdBtn")?.addEventListener("click", (e) => {
  e.preventDefault();
  showIshciId();
});
document.getElementById("idInput")?.addEventListener("keypress", (e) => {
  if (e.key === "Enter") { e.preventDefault(); showIshciId(); }
});

/* inline onclick üçün global */
window.showIshciId = showIshciId;
</script>



<script>
  const CSRF = document.querySelector("meta[name='csrf-token']")?.content || "";
  const ENDPOINT = "/people/invite-partner";

  document.addEventListener("DOMContentLoaded", () => {
    const btn = document.querySelector("#ishciId #inviteButton");
    if (!btn) return;

    btn.addEventListener("click", async () => {
      const userId = document.getElementById("hiddenUserId")?.value?.trim();
      if (!userId) return (window.alertModal?.("Error occured","error") ?? alert("Error occured"));

      btn.disabled = true;
      try {
        const res = await fetch(ENDPOINT, {
          method: "POST",
          headers: {
            "Content-Type": "application/json",
            "CSRF-Token": CSRF,
          },
          body: JSON.stringify({ id: userId }),
        });

        let data = null; try { data = await res.json(); } catch {}

        if (res.ok) {
          (window.alertModal?.(data?.message || "User added successfully") ?? alert("User added successfully"));
          document.getElementById("ishciId")?.classList.add("hidden");
          const ov = document.getElementById("overlay");
          if (ov) ov.style.display = "none";
          location.reload();
        } else {
          (window.alertModal?.(data?.message || "Error occured","error") ?? alert(data?.message || "Error occured"));
        }
      } catch (err) {
        (window.alertModal?.("Server error","error") ?? alert("Server error"));
      } finally {
        btn.disabled = false;
      }
    });
  });
</script>


<script>
(function () {
  const CSRF = document.querySelector('meta[name="csrf-token"]')?.content || "";

  window.currentDutyId = null;
  window.currentDutyRowSel = null;

  window.openVezifeSilPop = function (dutyId, rowSelector) {
    window.currentDutyId = dutyId;
    window.currentDutyRowSel = rowSelector || null;

    const pop = document.getElementById("VezifeSilPop");
    if (pop) pop.classList.remove("hidden");

    const ov = document.getElementById("overlay");
    if (ov) ov.classList.remove("hidden");
  };

  // Popup bağla (Xeyr düyməsində var)
  window.deletePopUp = function () {
    const pop = document.getElementById("VezifeSilPop");
    if (pop) pop.classList.add("hidden");

    const ov = document.getElementById("overlay");
    if (ov) ov.classList.add("hidden");

    window.currentDutyId = null;
    window.currentDutyRowSel = null;
  };

  window.toggleEmail = function () {
    if (!window.currentDutyId) {
      alert("Silinəcək vəzifə tapılmadı");
      return;
    }
    performDutyDelete(window.currentDutyId);
  };

  function removeDutyRowLocally(dutyId) {
    if (window.currentDutyRowSel) {
      const rowEl = document.querySelector(window.currentDutyRowSel);
      if (rowEl) {
        rowEl.remove();
        return true;
      }
    }
    const row = document.querySelector(`[data-duty-row="${dutyId}"]`);
    if (row) {
      row.remove();
      return true;
    }
    return false;
  }

  // Backend-ə silmə istəyi
  function performDutyDelete(dutyId) {
    const pop = document.getElementById("VezifeSilPop");
    if (pop) pop.classList.add("hidden");
    const ov = document.getElementById("overlay");
    if (ov) ov.classList.add("hidden");

    fetch("/rbac/rbacDeletes/delete", {
      method: "POST",
      headers: {
        "Content-Type": "application/json",
        "CSRF-Token": CSRF,
      },
      body: JSON.stringify({ id: dutyId }),
    })
      .then(async (res) => {
        const data = await res.json().catch(() => ({}));
        if (!res.ok) throw new Error(data?.message || "Delete failed");

        // OTP tələb olunursa — OTP popup aç
        if (data?.otpRequired) {
          if (typeof Otp === "function" && data.email && data.tempDeleteId) {
            Otp(data.email, data.tempDeleteId, {
              url: "/rbac/rbacAccept/delete",
              title: "Vəzifə Silmə",
              formType: "deleteDuty",
              submitText: "Təsdiqlə",
              cancelText: "Ləğv et",
              onSuccess: function () {
                // OTP təsdiq olunduqdan sonra sətiri sil
                if (!removeDutyRowLocally(dutyId)) location.reload();
              },
            });
          } else {
            alert("OTP açıla bilmədi");
          }
          return;
        }

        // Uğurlu delete → sətiri cədvəldən çıxar
        if (data?.success) {
          if (!removeDutyRowLocally(dutyId)) {
            // əgər sətri tapa bilmədiksə, fallback olaraq reload et
            location.reload();
          }
          return;
        }

        throw new Error(data?.message || "Xəta baş verdi");
      })
      .catch((err) => {
        console.error("Duty delete error:", err);
        alert(err.message || "Server xətası");
      })
      .finally(() => {
        window.currentDutyId = null;
        window.currentDutyRowSel = null;
      });
  }


})();
</script>


