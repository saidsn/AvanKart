    <script>
      // CSRF Token'i alma - DÜZƏLDİLMİŞ
      function getCSRFToken() {
        const metaToken = document
          .querySelector('meta[name="csrf-token"]')
          ?.getAttribute("content");
        if (metaToken && metaToken.trim() !== "") {
          return metaToken;
        }

        const inputToken = document.querySelector('input[name="_csrf"]')?.value;
        if (inputToken && inputToken.trim() !== "") {
          return inputToken;
        }

        console.warn("CSRF token tapılmadı");
        return null;
      }

      function openCreateMukafatModal() {
        const modal = document.getElementById("createMukafatModal");
        modal.classList.remove("hidden");
        loadCardsForSelect();
      }

      function closeCreateMukafatModal() {
        const modal = document.getElementById("createMukafatModal");
        modal.classList.add("hidden");
        document.getElementById("createMukafatForm").reset();
      }

      async function loadCardsForSelect() {
        try {
          const csrfToken = getCSRFToken();
          const response = await fetch(
            "/emeliyyatlar/avankart/mukafat/api/cards",
            {
              headers: {
                "CSRF-Token": csrfToken,
              },
            }
          );

          const select = document.querySelector('select[name="forCard"]');
          select.innerHTML = '<option value="">Kart seçin</option>';

          if (!response.ok) {
            throw new Error("Kartlar yükləniləmədi");
          }

          const cards = await response.json();

          if (!cards || cards.length === 0) {
            // Mock kartlar
            const mockCards = [
              { _id: "1", name: "Standart Kart" },
              { _id: "2", name: "Premium Kart" },
              { _id: "3", name: "Gold Kart" },
              { _id: "4", name: "Platinum Kart" },
            ];

            mockCards.forEach((card) => {
              const option = document.createElement("option");
              option.value = card._id;
              option.textContent = card.name;
              select.appendChild(option);
            });
          } else {
            cards.forEach((card) => {
              const option = document.createElement("option");
              option.value = card._id;
              option.textContent = card.name;
              select.appendChild(option);
            });
          }
        } catch (error) {
          console.error("Kartlar yüklənərkən xəta:", error);
          // Xəta zamanı mock kartlar
          const select = document.querySelector('select[name="forCard"]');
          select.innerHTML = '<option value="">Kart seçin</option>';

          const mockCards = [
            { _id: "1", name: "Standart Kart" },
            { _id: "2", name: "Premium Kart" },
            { _id: "3", name: "Gold Kart" },
            { _id: "4", name: "Platinum Kart" },
          ];

          mockCards.forEach((card) => {
            const option = document.createElement("option");
            option.value = card._id;
            option.textContent = card.name;
            select.appendChild(option);
          });
        }
      }

      async function submitMukafatForm(event) {
        event.preventDefault();

        const form = event.target;
        const formData = new FormData(form);
        const csrfToken = getCSRFToken();

        if (!csrfToken) {
          showNotification(
            "CSRF token tapılmadı. Səhifəni yeniləyin.",
            "error"
          );
          return;
        }

        formData.set("_csrf", csrfToken);

        // Loading state
        const submitBtn = form.querySelector('button[type="submit"]');
        const originalText = submitBtn.textContent;
        submitBtn.textContent = "Yaradılır...";
        submitBtn.disabled = true;

        try {
          const endpoint = "api/emeliyyatlar/avankart/mukafat/create";

          const response = await fetch(endpoint, {
            method: "POST",
            headers: {
              "x-csrf-token": csrfToken,
            },
            body: formData, // FormData olaraq göndər, JSON deyil
          });

          const responseText = await response.text();

          if (!response.ok) {
            if (
              responseText.startsWith("<!DOCTYPE") ||
              responseText.startsWith("<html")
            ) {
              throw new Error(
                "Server xətası: HTML cavab alındı - Status: " + response.status
              );
            }

            try {
              const errorData = JSON.parse(responseText);
              throw new Error(
                errorData.message || `HTTP xətası! status: ${response.status}`
              );
            } catch (parseError) {
              throw new Error(
                `HTTP xətası! status: ${response.status} - Mesaj: ${responseText}`
              );
            }
          }

          let result;
          try {
            result = JSON.parse(responseText);
          } catch (parseError) {
            console.error("JSON parse xətası:", parseError);
            throw new Error("Yanlış JSON cavab: " + responseText);
          }

          showNotification("Mükafat uğurla yaradıldı!", "success");
          closeCreateMukafatModal();

          // Mock data formatında tabloya əlavə et
          if (result.mockData) {
            addNewMukafatToTable(result.mockData);
          } else {
            // Səhifəni yenilə
            setTimeout(() => {
              window.location.reload();
            }, 1000);
          }
        } catch (error) {
          console.error("Mükafat yaradılarkən xəta:", error);
          showNotification(
            "Mükafat yaradılarkən xəta baş verdi: " + error.message,
            "error"
          );
        } finally {
          submitBtn.textContent = originalText;
          submitBtn.disabled = false;
        }
      }

      function addNewMukafatToTable(mockData) {
        const table = document.querySelector("#myTable tbody");
        if (!table) return;

        const newRow = document.createElement("tr");
        newRow.className =
          "border-none text-[12px] text-secondary-text hover:bg-container-2 cursor-pointer";

        const statusClass =
          mockData.status === "Davam edir"
            ? "bg-yellow-100 text-yellow-800 dark:bg-yellow-900 dark:text-yellow-200"
            : "bg-green-100 text-green-800 dark:bg-green-900 dark:text-green-200";

        newRow.innerHTML = `
        <td class="py-3 px-4 rounded-l-lg">${mockData.invoice}</td>
        <td class="py-3 px-4">${mockData.customers}</td>
        <td class="py-3 px-4">${mockData.transactions}</td>
        <td class="py-3 px-4">${mockData.amount.toFixed(2)} AZN</td>
        <td class="py-3 px-4">${mockData.date}</td>
        <td class="py-3 px-4 rounded-r-lg">
            <span class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium ${statusClass}">
                ${mockData.status}
            </span>
        </td>
    `;

        table.insertBefore(newRow, table.firstChild);
        updateMukafatCount();
      }

      function updateMukafatCount() {
        const countElement = document.querySelector(
          ".text-\\[15px\\].text-messages.font-medium"
        );
        if (countElement) {
          const currentText = countElement.textContent;
          const match = currentText.match(/Mükafat $$(\d+)$$/);
          if (match) {
            const currentCount = parseInt(match[1]);
            countElement.textContent = `Mükafat (${currentCount + 1})`;
          }
        }
      }

      function showNotification(message, type) {
        const existingNotifications = document.querySelectorAll(
          ".custom-notification"
        );
        existingNotifications.forEach((notification) => notification.remove());

        const notification = document.createElement("div");
        notification.className = `custom-notification fixed top-4 right-4 p-4 rounded-lg text-white z-[1000] shadow-lg ${
          type === "success" ? "bg-green-500" : "bg-red-500"
        }`;
        notification.textContent = message;

        document.body.appendChild(notification);

        setTimeout(() => {
          notification.style.opacity = "0";
          notification.style.transition = "opacity 0.3s ease-out";
          setTimeout(() => notification.remove(), 300);
        }, 5000);
      }

      document.addEventListener("click", function (event) {
        const createModal = document.getElementById("createMukafatModal");
        const filterModal = document.getElementById("filterPop");

        if (createModal && !createModal.classList.contains("hidden")) {
          if (event.target === createModal) {
            closeCreateMukafatModal();
          }
        }

        if (filterModal && !filterModal.classList.contains("hidden")) {
          if (event.target === filterModal) {
            openFilterModal();
          }
        }
      });

      document.addEventListener("keydown", function (event) {
        if (event.key === "Escape") {
          closeCreateMukafatModal();
          const filterModal = document.getElementById("filterPop");
          if (filterModal && !filterModal.classList.contains("hidden")) {
            openFilterModal();
          }
        }
      });

      function openFilterModal() {
        const modal = document.getElementById("filterPop");
        modal.classList.toggle("hidden");
      }

      function toggleDropdown_years() {
        const dropdown = document.getElementById("dropdown_years");
        dropdown.classList.toggle("hidden");
      }

      function toggleDropdown_months() {
        const dropdown = document.getElementById("dropdown_months");
        dropdown.classList.toggle("hidden");
      }

      function clearFilters() {
        document.getElementById("filterForm").reset();
        if (typeof jQuery !== "undefined" && $("#slider-range").length) {
          $("#slider-range").slider("values", [0, 1000]);
          $("#min-value").text("0 AZN");
          $("#max-value").text("1000 AZN");
        }
      }

      function applyFilters() {
        openFilterModal();
      }

      document.addEventListener("DOMContentLoaded", function () {
        console.log("CSRF Token:", getCSRFToken());

        if (typeof jQuery !== "undefined" && $("#slider-range").length) {
          $("#slider-range").slider({
            range: true,
            min: 0,
            max: 1000,
            values: [0, 1000],
            slide: function (event, ui) {
              $("#min-value").text(ui.values[0] + " AZN");
              $("#max-value").text(ui.values[1] + " AZN");
            },
          });
          $("#min-value").text($("#slider-range").slider("values", 0) + " AZN");
          $("#max-value").text($("#slider-range").slider("values", 1) + " AZN");
        }

        document.addEventListener("click", function (event) {
          const yearsDropdown = document.getElementById("dropdown_years");
          const monthsDropdown = document.getElementById("dropdown_months");
          const yearsButton = document.getElementById(
            "dropdownDefaultButton_years"
          );
          const monthsButton = document.getElementById(
            "dropdownDefaultButton_months"
          );

          if (
            yearsDropdown &&
            !yearsDropdown.classList.contains("hidden") &&
            !yearsDropdown.contains(event.target) &&
            !yearsButton.contains(event.target)
          ) {
            yearsDropdown.classList.add("hidden");
          }

          if (
            monthsDropdown &&
            !monthsDropdown.classList.contains("hidden") &&
            !monthsDropdown.contains(event.target) &&
            !monthsButton.contains(event.target)
          ) {
            monthsDropdown.classList.add("hidden");
          }
        });
      });
    </script>
  </body>
</html>