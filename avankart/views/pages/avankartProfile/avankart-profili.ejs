        <% scripts = `
  <script src="/js/datatables/datatables.min.js"></script><script src="/js/Avankart/Avankart Profili/popup2.js"></script>
    <script src="/js/Avankart/Avankart Profili/avankartProfili.js"></script>
  <script src="/js/Avankart Profili/Popup.js"></script>
  <script>
    // Rekvizit modal handlers
    document.addEventListener('DOMContentLoaded', function() {
      const openBtn = document.getElementById('openRekvizitModalBtn');
      const openBtn2 = document.getElementById('openRekvizitModalBtn2');
      const closeBtn = document.getElementById('closeRekvizitModal');
      const cancelBtn = document.getElementById('cancelRekvizitBtn');
      const modal = document.getElementById('rekvizitModal');
      const form = document.getElementById('rekvizitForm');

      function openModal() {
        if (modal) {
          modal.classList.remove('hidden');
        }
      }

      function closeModal() {
        if (modal) {
          modal.classList.add('hidden');
          if (form) form.reset();
        }
      }

      if (openBtn) {
        openBtn.addEventListener('click', openModal);
      }

      if (openBtn2) {
        openBtn2.addEventListener('click', openModal);
      }

      if (closeBtn) {
        closeBtn.addEventListener('click', closeModal);
      }

      if (cancelBtn) {
        cancelBtn.addEventListener('click', closeModal);
      }

      // Close modal when clicking outside
      if (modal) {
        modal.addEventListener('click', function(e) {
          if (e.target === modal) {
            closeModal();
          }
        });
      }

      // Form submission
      if (form) {
        form.addEventListener('submit', function(e) {
          e.preventDefault();
          
          const formData = new FormData(form);
          const data = Object.fromEntries(formData.entries());
          
          // Validate required fields
          if (!data.company_name || !data.bank_name) {
            alert('Şirkətin adı və Bankın adı mütləq doldurulmalıdır');
            return;
          }
          
          // Make API call to create rekvizit
          fetch('/api/avankart/rekvizit', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json',
              'X-CSRF-Token': data._csrf
            },
            body: JSON.stringify(data)
          })
          .then(response => response.json())
          .then(result => {
            if (result.success) {
              alert('Rekvizit uğurla əlavə edildi');
              closeModal();
              // Reload the rekvizitler list
              loadRekvizitler();
            } else {
              alert('Xəta baş verdi: ' + (result.message || 'Bilinməyən xəta'));
            }
          })
          .catch(error => {
            console.error('Error:', error);
            alert('Xəta baş verdi. Zəhmət olmasa yenidən cəhd edin.');
          });
        });
      }
    });
  </script>
  `; %>