    <script>
        // Rekvizit Map functionality
        let rekvizitMap;
        let rekvizitGeocoder;
        let RekvizitAdvancedMarkerElement;
        let rekvizitCurrentMarker = null;

        window.initRekvizitMap = async function () {
            rekvizitMap = new google.maps.Map(document.getElementById("rekvizitMap"), {
                center: { lat: 40.4093, lng: 49.8671 }, // Bakı mərkəzi
                zoom: 13,
                mapId: "c85dc535485567877bdc6fc9"
            });

            rekvizitGeocoder = new google.maps.Geocoder();
            RekvizitAdvancedMarkerElement = google.maps.marker.AdvancedMarkerElement;

            // Xəritədə klik edildikdə marker yaradılacaq
            rekvizitMap.addListener("click", (event) => {
                const latlng = {
                    lat: event.latLng.lat(),
                    lng: event.latLng.lng()
                };

                // Koordinatları inputa yaz
                const coordString = latlng.lat.toFixed(6) + ', ' + latlng.lng.toFixed(6);
                document.getElementById(window.rekvizitForAddress ?? 'rekvizitCoordinates').value = coordString;
                
                // Koordinat göstəricini yenilə
                const coordinatesDisplay = document.getElementById('coordinatesDisplay');
                const coordinatesText = document.getElementById('coordinatesText');
                if (coordinatesDisplay && coordinatesText) {
                    coordinatesText.textContent = coordString;
                    coordinatesDisplay.style.display = 'block';
                }

                // Köhnə markeri sil
                if (rekvizitCurrentMarker) rekvizitCurrentMarker.map = null;

                // Yeni marker əlavə et
                rekvizitCurrentMarker = new RekvizitAdvancedMarkerElement({
                    map: rekvizitMap,
                    position: latlng
                });

                // Ünvan məlumatını çək
                rekvizitGeocoder.geocode({ location: latlng }, (results, status) => {
                    if (status === "OK" && results[0]) {
                        document.getElementById(window.rekvizitForHidden ?? 'rekvizitAddress').value = 
                            results[0].formatted_address;
                    } else {
                        document.getElementById(window.rekvizitForHidden ?? 'rekvizitAddress').value = 
                            "Ünvan tapılmadı";
                    }
                });
                closeRekvizitMapModal();
            });
        };

        // Rekvizit map modal funksiyaları  
        function openRekvizitMapModal(addressId = null, coordinatesId = null) {
            window.rekvizitForHidden = addressId;
            window.rekvizitForAddress = coordinatesId;
            
            const mapModal = document.getElementById("rekvizitMapModal");
            mapModal.classList.remove("hidden");
            
            // Xəritəni yenidən ölçüləndir
            setTimeout(() => {
                if (rekvizitMap) {
                    google.maps.event.trigger(rekvizitMap, 'resize');
