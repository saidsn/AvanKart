<script>
    // Progressive navigation helper function
    function showOnly(selectors) {
        // Hide all sections with data-ops-section attribute
        const allSections = document.querySelectorAll('[data-ops-section]');
        allSections.forEach(section => {
            section.classList.add('ops-hidden');
        });

        // Show only the specified selectors
        if (Array.isArray(selectors)) {
            selectors.forEach(selector => {
                const element = document.querySelector(selector);
                if (element) {
                    element.classList.remove('ops-hidden');
                }
            });
        } else {
            const element = document.querySelector(selectors);
            if (element) {
                element.classList.remove('ops-hidden');
            }
        }
    }

    // Initialize page to show only cards on DOM content loaded
    document.addEventListener('DOMContentLoaded', function () {
        showOnly(['#ops-cards']);
    });
</script>



