<div class="w-full" id="eqaime-root" data-company-id="<%= company._id %>">
    <meta name="company-id" content="<%= (company && company._id) ? company._id : '' %>">
    <script>window.CSRF_TOKEN = '<%= csrfToken %>';</script>
    <style>
        .ops-hidden {
            display: none !important;
        }
    </style>
    <div class="w-full relative">
        <!-- Arxa fon şəkli -->
        <img id="company-cover"
            src="<%= company.daxili_cover_image_path || company.xarici_cover_image_path || '/images/uzv-sirket/sirketMelumatlari/bgImage.svg' %>"
            alt="Background Image" class="w-full" />

        <!-- Logo şəkli: Yarı hündürlüyü qədər yuxarı çıxsın -->
        <img id="company-avatar" class="absolute w-24 h-24 z-10 left-5 -bottom-12"
            src="<%= company.profile_image_path || '/images/Avankart/sirketler/logo.svg' %>" alt="Logo" />
    </div>
    <div class="flex mt-[42px] w-[100%]">
        <div class="w-[25%] px-3 border-r border-stroke pt-[12px] min-h-screen">
            <% if (pendingPatch) { %>
                <div class="bg-sidebar-bg py-[10px] px-[12px] mb-[8px]">
                    <div class="flex justify-between items-center">
                        <p class="text-12px font-medium text-messages pb-1">
                            Dəyişikliyi təsdiqlə
                        </p>
                        <div class="flex gap-2">
                            <button
                                class="border-[1px] border-green-500 bg-green-500 text-white w-[60px] h-[27px] flex justify-center items-center rounded-[100px] hover:cursor-pointer hover:bg-green-600 text-[12px] font-medium"
                                onclick="confirmChanges('<%= company._id %>')">
                                Təsdiqlə
                            </button>
                            <button
                                class="border-[1px] border-red-500 bg-red-500 text-white w-[60px] h-[27px] flex justify-center items-center rounded-[100px] hover:cursor-pointer hover:bg-red-600 text-[12px] font-medium"
                                onclick="rejectChanges('<%= company._id %>')">
                                Rədd et
                            </button>
                        </div>
                    </div>

                    <div class="mt-2 text-[11px] text-messages">
                        <div class="mb-1">
                            <strong>Ad:</strong>
                            <span class="text-gray-500">
                                <%= company.sirket_name || '—' %>
                            </span> →
                            <span class="text-green-600">
                                <%= pendingPatch.sirket_name || '—' %>
                            </span>
                        </div>
                        <div class="mb-1">
                            <strong>Email:</strong>
                            <span class="text-gray-500">
                                <%= company.email && company.email[0] || '—' %>
                            </span> →
                            <span class="text-green-600">
                                <%= pendingPatch.email && pendingPatch.email[0] || '—' %>
                            </span>
                        </div>
                        <div class="mb-1">
                            <strong>Komissiya:</strong>
                            <span class="text-gray-500">
                                <%= company.commission_percentage || '—' %>%
                            </span> →
                            <span class="text-green-600">
                                <%= pendingPatch.commission_percentage || '—' %>%
                            </span>
                        </div>
                        <div class="mb-1">
                            <strong>Təsvir:</strong>
                            <span class="text-gray-500">
                                <%= company.description || '—' %>
                            </span> →
                            <span class="text-green-600">
                                <%= pendingPatch.description || '—' %>
                            </span>
                        </div>
                    </div>
                </div>
                <% } %>

                    <div class="bg-sidebar-bg py-[5.5px] px-[12px] mb-[8px]">
                        <span class="text-[12px] font-normal text-messages opacity-50 pr-[4px]">Üzv olduğu
                            tarix:</span>
                        <span id="company-created" class="text-[12px] font-medium texy-mess">
                            <%= company.createdAt ? new Date(company.createdAt).toLocaleDateString('az-AZ', {
                                day: '2-digit' , month: '2-digit' , year: 'numeric' }) + ' ' + new
                                Date(company.createdAt).toLocaleTimeString('az-AZ', { hour: '2-digit' ,
                                minute: '2-digit' }) : '—' %>
                        </span>
                    </div>
                    <!-- Your existing HTML remains the same -->
                    <div class="bg-sidebar-bg py-[5.5px] px-[12px] mb-[8px]">
                        <span class="text-[12px] font-normal text-messages opacity-50 pr-[4px]">Son düzəliş
                            tarixi:</span>
                        <span id="company-updated" class="text-[12px] font-medium text-messages">
                            <%= company.updatedAt ? new Date(company.updatedAt).toLocaleDateString('az-AZ', {
                                day: '2-digit' , month: '2-digit' , year: 'numeric' }) + ' ' + new
                                Date(company.updatedAt).toLocaleTimeString('az-AZ', { hour: '2-digit' ,
                                minute: '2-digit' }) : '—' %>
                        </span>
                    </div>
                    <div class="bg-sidebar-bg py-[5.5px] px-[12px] mb-[8px]">
                        <span class="text-[12px] font-normal text-messages opacity-50 pr-[4px]">Yaradan
                            şəxs</span>
                        <span id="company-creator" class="text-[12px] font-medium text-messages">
                            <%= String(company.creator_id || '—' ) %>
                        </span>
                    </div>
                    <div class="bg-sidebar-bg py-[5.5px] px-[12px] mb-[8px]">
                        <span class="text-[12px] font-normal text-messages opacity-50 pr-[4px]">Komissiya faizi</span>
                        <span id="company-commission" class="text-[12px] font-medium text-messages">
                            <%= company.commission_percentage || '—' %>%
                        </span>
                    </div>
                    <div class="bg-sidebar-bg py-[5.5px] px-[12px] mb-[8px]">
                        <span class="text-[12px] font-normal text-messages opacity-50 pr-[4px]">Kəşbek faizi</span>
                        <span id="company-cashback" class="text-[12px] font-medium text-messages">
                            <%= company.cashback_percentage || '—' %>%
                        </span>
                    </div>
                    <div class="bg-sidebar-bg py-[5.5px] px-[12px] mb-[8px]">
                        <span class="text-[12px] font-normal text-messages opacity-50 pr-[4px]">Məsul şəxs</span>
                        <span id="company-authorized" class="text-[12px] font-medium text-messages">
                            <%= company.authorized_person && company.authorized_person.name || '—' %>
                        </span>
                    </div>
                    <div class="bg-sidebar-bg py-[5.5px] px-[12px] mb-[8px]">
                        <span class="text-[12px] font-normal text-messages opacity-50 pr-[4px]">Kateqoriya</span>
                        <span id="company-category" class="text-[12px] font-medium text-messages">
                            <%= company.sirket_category || '—' %>
                        </span>
                    </div>
                    <% if (company.services && company.services.length> 0) { %>
                        <div class="bg-sidebar-bg py-[5.5px] px-[12px] mb-[8px]">
                            <span class="text-[12px] font-normal text-messages opacity-50 pr-[4px]">Xidmətlər</span>
                            <div id="company-services" class="flex flex-wrap gap-1 mt-1">
                                <% company.services.forEach(function(service) { %>
                                    <span class="text-[10px] bg-primary text-white px-2 py-1 rounded-full">
                                        <%= service %>
                                    </span>
                                    <% }); %>
                            </div>
                        </div>
                        <% } %>

                            <div class="flex justify-center items-center gap-3 mt-2">
                                <button id="inside-edit-btn" data-id="<%= company._id %>"
                                    class="border border-surface-variant w-full h-[34px] flex gap-[10px] justify-center items-center rounded-full hover:cursor-pointer hover:text-focus text-[12px] font-medium text-messages">
                                    <span class="icon iconex-edit-1-1 text-sm text-messages"></span>
                                    Redaktə et
                                </button>
                                <div id="wrapper" class="relative inline-block text-left z-25">
                                    <div onclick="toggleDropdown(this)"
                                        class="relative border w-[34px] h-[34px] rounded-full flex items-center justify-center flex-none">
                                        <button id="threeDotBtn"
                                            class="icon stratis-dot-vertical cursor-pointer text-messages">
                                            ⋮
                                        </button>
                                    </div>

                                    <div class="hidden absolute right-[-12px] max-w-[244px] z-25 dropdown-menu">
                                        <div class="relative h-[8px]">
                                            <div
                                                class="absolute top-1/2 right-4 w-3 h-3 bg-menu rotate-45 border-l-[.5px] border-t-[.5px] z-50 border-[.5px] border-stroke">
                                            </div>
                                        </div>

                                        <div
                                            class="rounded-xl shadow-lg bg-menu overflow-hidden relative z-50 border-[.5px] border-stroke">
                                            <div class="py-[3.5px] text-sm">
                                                <div onclick="toggleDeAktivModal()"
                                                    class="flex items-center gap-2 px-4 py-[3.5px] hover:bg-error-hover cursor-pointer">
                                                    <span
                                                        class="icon stratis-minus-circle-contained text-error text-[13px]"></span>
                                                    <span
                                                        class="font-medium text-error text-[13px] whitespace-nowrap">Deaktiv
                                                        et</span>
                                                </div>
                                                <div class="h-[.5px] bg-stroke my-1"></div>
                                                <div onclick="toggleSilinmeMuracietPopUp()"
                                                    class="flex items-center gap-2 px-4 py-[3.5px] hover:bg-error-hover cursor-pointer">
                                                    <span class="icon stratis-trash-01 text-error text-[13px]"></span>
                                                    <span
                                                        class="font-medium text-error text-[13px] whitespace-nowrap">Silinmə
                                                        üçün müraciət et</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="mt-[16px]">
                                <div class="flex justify-between items-center">
                                    <p id="company-name-display" class="text-[22px] font-semibold text-messages">
                                        <%= company.sirket_name || '—' %>
                                    </p>
                                    <button id="company-status-badge"
                                        class="bg-sidebar-bg w-[60px] h-[25px] flex gap-[8px] items-center rounded-[100px] text-[12px] font-medium text-messages">
                                        <span
                                            class="w-[6px] h-[6px] rounded-full shrink-0 ml-2 <%= company.company_status === 0 ? 'bg-[#00A3FF]' : company.company_status === 1 ? 'bg-gray-400' : company.company_status === 3 ? 'bg-yellow-500' : 'bg-red-500' %>"></span>
                                        <%= company.company_status===0 ? 'Aktiv' : company.company_status===1
                                            ? 'Deaktiv' : company.company_status===3 ? 'Silinmə gözləyir' : 'Silinmiş'
                                            %>
                                    </button>
                                </div>
                            </div>
                            <div class="mt-1">
                                <span class="py-[2.5px] flex items-center gap-[4px]">
                                    <span class="font-normal text-[12px] text-messages opacity-65">ID:</span>
                                    <span id="company-id-display" class="font-normal text-[12px] text-messages">
                                        <%= company.sirket_id || '—' %>
                                    </span>
                                    <span class="icon iconex-copy-1 w-[12px] h-[15px] ml-[6px]"></span>
                                </span>
                            </div>
                            <p id="company-description"
                                class="pt-2 font-normal text-[12px] text-messages opacity-65 pb-[20px] border-b-[0.5px] border-[#0000001A]">
                                <%= company.description || '—' %>
                            </p>
                            <div class="flex gap-4 flex-col pb-[20px] border-b-[0.5px] pt-4 border-[#0000001A]">
                                <div class="flex gap-[11px] items-center text-messages">
                                    <span class="mt-[6px] icon iconex-bookmark-1 !w-[13px] !h-[17px]"></span><span
                                        class="opacity-65 text-[13px] font-normal">Şirkət</span>
                                </div>
                                <div id="company-name" class="hidden">
                                    <%= company.sirket_name || '' %>
                                </div>
                                <div id="company-id" class="hidden">
                                    <%= company.sirket_id || '' %>
                                </div>
                                <div class="flex gap-[11px] items-center text-messages">
                                    <span class="icon iconex-location-1 !w-[15px] !h-[17px]"></span><span
                                        id="company-address" class="opacity-65 text-[13px] font-normal">
                                        <%= company.address || '—' %>
                                    </span>
                                </div>
                                <% if (company.phone && company.phone.length> 0) { %>
                                    <% company.phone.forEach(function(phone, index) { %>
                                        <div class="flex gap-[11px] items-center text-messages">
                                            <span class="icon iconex-calling-1 !w-[16px] !h-[16px]"></span>
                                            <a href="tel:+<%= phone.prefix %><%= phone.number %>"
                                                class="opacity-65 text-[13px] font-normal hover:text-focus">+<%=
                                                    phone.prefix %>
                                                    <%= phone.number %></a>
                                        </div>
                                        <% }); %>
                                            <% } else { %>
                                                <div class="flex gap-[11px] items-center text-messages">
                                                    <span class="icon iconex-calling-1 !w-[16px] !h-[16px]"></span><span
                                                        class="opacity-65 text-[13px] font-normal">—</span>
                                                </div>
                                                <% } %>
                                                    <div class="flex gap-[11px] items-center text-messages">
                                                        <span class="icon iconex-case-1 !w-[17px] !h-[16px]"></span>
                                                        <% if (company.email && company.email.length> 0) { %>
                                                            <a href="mailto:<%= company.email[0] %>" id="company-email"
                                                                class="opacity-65 text-[13px] font-normal hover:text-focus">
                                                                <%= company.email[0] %>
                                                            </a>
                                                            <% } else { %>
                                                                <span
                                                                    class="opacity-65 text-[13px] font-normal">—</span>
                                                                <% } %>
                                                    </div>
                            </div>
                            <div class="pt-4 pb-[20px] border-b-[0.5px] border-[#0000001A]">
                                <p class="text-[11px] font-medium opacity-50 text-messages pb-2">
                                    Kartlar (<span id="company-cards-count">
                                        <%= company.cards ? company.cards.length : 0 %>
                                    </span>)
                                </p>
                                <div id="company-cards" class="flex flex-wrap gap-3">
                                    <% if (company.cards && company.cards.length> 0) { %>
                                        <% company.cards.forEach(function(card) { %>
                                            <div
                                                class="w-[84px] h-[27px] flex gap-1 items-center justify-center bg-[#FFBC0D] rounded-full">
                                                <img class="w-[16px] h-[16px]"
                                                    src="/images/muessise melumatlari images/ForkKnife.svg"
                                                    alt="Card" />
                                                <span class="font-normal text-[12px] text-messages">
                                                    <%= card.name || 'Kart' %>
                                                </span>
                                            </div>
                                            <% }); %>
                                                <% } else { %>
                                                    <span class="text-[12px] text-messages opacity-50">Kart
                                                        yoxdur</span>
                                                    <% } %>
                                </div>
                            </div>
                            <div class="pt-4 pb-5 border-b-[0.5px] border-[#0000001A]">
                                <p class="text-[11px] font-medium opacity-50 text-messages pb-2">
                                    İş günləri
                                </p>
                                <div id="company-schedule" class="flex gap-1 flex-wrap">
                                    <% if (company.schedule && Object.keys(company.schedule).length> 0) { %>
                                        <% Object.entries(company.schedule).forEach(function([day, time]) { %>
                                            <div class="flex items-center">
                                                <img class="w-[34px] h-[34px]"
                                                    src="/images/muessise melumatlari images/Frame 427321302.svg"
                                                    alt="<%= day %>" />
                                                <p class="text-[12px] font-medium opacity-65 text-messages pl-3">
                                                    <%= day %>: <%= time || '—' %>
                                                </p>
                                            </div>
                                            <% }); %>
                                                <% } else { %>
                                                    <span class="text-[12px] text-messages opacity-50">İş günləri təyin
                                                        edilməyib</span>
                                                    <% } %>
                                </div>
                            </div>
                            <div class="pt-4 pb-5 border-b-[0.5px] border-[#0000001A]">
                                <p class="text-[11px] font-medium opacity-50 text-messages pb-2">
                                    Sosial media
                                </p>
                                <div id="company-social" class="flex flex-col">
                                    <% if (company.social && (company.social instanceof Map ? company.social.size> 0 :
                                        Object.keys(company.social).length > 0)) { %>
                                        <% const socialEntries=company.social instanceof Map ?
                                            Array.from(company.social.entries()) : Object.entries(company.social); %>
                                            <% socialEntries.forEach(function([platform, url]) { %>
                                                <div
                                                    class="p-3 flex items-center justify-between border-b-[0.5px] border-[#0000001A] hover:bg-table-hover">
                                                    <div class="flex items-center gap-2">
                                                        <img src="/images/muessise melumatlari images/devicon_<%= platform.toLowerCase() %>.svg"
                                                            alt="<%= platform %>" />
                                                        <span class="text-[13px] font-medium text-messages opacity-65">
                                                            <%= platform %>
                                                        </span>
                                                    </div>
                                                    <a href="<%= url %>" target="_blank"
                                                        class="icon stratis-link-external text-xs hover:text-focus"></a>
                                                </div>
                                                <% }); %>
                                                    <% } else { %>
                                                        <span class="text-[12px] text-messages opacity-50 p-3">Sosial
                                                            media linkləri yoxdur</span>
                                                        <% } %>
                                </div>
                            </div>
                            <div class="pt-4 pb-5 border-b-[0.5px] border-[#0000001A] mb-[17px]">
                                <p class="text-[11px] font-medium opacity-50 text-messages pb-2">
                                    Web sayt
                                </p>
                                <div id="company-websites" class="flex flex-col">
                                    <% if (company.website && company.website.length> 0) { %>
                                        <% company.website.forEach(function(url) { %>
                                            <div class="p-3 flex gap-2 items-center hover:bg-table-hover">
                                                <div class="icon iconex-link-1 w-[9px] h-[17px]"></div>
                                                <a href="<%= url %>" target="_blank"
                                                    class="text-[13px] font-medium text-messages opacity-65 hover:text-focus">
                                                    <%= url %>
                                                </a>
                                            </div>
                                            <% }); %>
                                                <% } else { %>
                                                    <div class="p-3 flex gap-2 items-center">
                                                        <div class="icon iconex-link-1 w-[9px] h-[17px]"></div>
                                                        <span
                                                            class="text-[13px] font-medium text-messages opacity-65">—</span>
                                                    </div>
                                                    <% } %>
                                </div>
                            </div>
        </div>
        <div class="max-w-[75%] px-5 w-[100%]">
            <div class="flex flex-col">
                <div class="flex flex-col gap-4">
                    <div class="border-b border-stroke inline-block">
                        <ul
                            class="inline-flex flex-wrap gap-5 -mb-px text-[13px] font-medium text-center text-gray-600">
                            <li>
                                <a href="#"
                                    class="main-tab-link inline-flex items-center justify-center py-2 rounded-t-lg text-messages border-b-2 border-messages"
                                    data-tab="dashboard">
                                    Dashboard
                                </a>
                            </li>
                            <li>
                                <a href="#"
                                    class="main-tab-link inline-flex items-center justify-center py-2 rounded-t-lg"
                                    data-tab="istifadeciler">
                                    İstifadəçilər
                                </a>
                            </li>
                            <li>
                                <a href="#"
                                    class="main-tab-link inline-flex items-center justify-center py-2 rounded-t-lg"
                                    data-tab="bildirisler">
                                    Bildirişlər
                                </a>
                            </li>
                            <li>
                                <a href="#"
                                    class="main-tab-link inline-flex items-center justify-center py-2 rounded-t-lg"
                                    data-tab="rekvizitler">
                                    Rekvizitlər
                                </a>
                            </li>
                            <li>
                                <a href="#"
                                    class="main-tab-link inline-flex items-center justify-center py-2 rounded-t-lg"
                                    data-tab="muqavileler">
                                    Müqavilələr
                                </a>
                            </li>
                            <li>
                                <a href="#"
                                    class="main-tab-link inline-flex items-center justify-center py-2 rounded-t-lg"
                                    data-tab="emeliyyatlar">
                                    Əməliyyatlar
                                </a>
                            </li>
                            <li>
                                <a href="#"
                                    class="main-tab-link inline-flex items-center justify-center py-2 rounded-t-lg"
                                    data-tab="sorgular">
                                    Sorğular
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>

                <div id="tab-content">
                    <div id="dashboard" class="main-tab-content">
                        <div class="w-full flex flex-col gap-3 py-3">
                            <div class="w-full px-4 py-4 rounded-lg bg-container-2 dark:bg-container-2-dark">
                                <div class="flex justify-between mb-5">
                                    <!-- Title Section -->
                                    <div
                                        class="relative h-[35px] pl-6 pr-10 flex items-center overflow-hidden -mt-[10px] flex-nowrap">
                                        <div
                                            class="absolute w-[22px] h-[23px] rotate-65 right-0 border-r border-secondary mt-[4px]">
                                        </div>
                                        <div
                                            class="font-medium text-[15px] whitespace-nowrap text-black dark:text-primary-text-color-dark">
                                            İşçilərin statistikası
                                        </div>
                                        <div
                                            class="absolute bottom-0 bg-[#07DBFA] w-[29px] h-[3px] -ml-2 z-[1] border-r-[3px] border-container-2 dark:border-container-2-dark border-before [clip-path:polygon(100%_0,100%_100%,0_100%,2px_0)]">
                                        </div>
                                        <div
                                            class="absolute bottom-0 w-[calc(100%-39px)] border-b border-secondary mb-[1px]">
                                        </div>
                                    </div>
                                    <!-- Dropdowns Section -->
                                    <div id="customScroll"
                                        class="relative items-center w-full overflow-hidden flex justify-end">
                                        <div class="absolute w-full  z-[1] border-b border-secondary !right-[19%]">
                                        </div>
                                        <div class="flex items-center w-full justify-end relative ">
                                            <div class="absolute w-full z-[1] border-b border-secondary">
                                            </div>
                                            <div
                                                class="flex justify-between min-w-96 py-1 px-1 rounded-full bg-menu dark:bg-menu-dark gap-2 items-center z-[2] border border-stroke dark:border-surface-variant-dark">
                                                <div
                                                    class="iscilerTab text-[12px] font-medium cursor-pointer text-messages dark:text-primary-text-color-dark hover:text-messages dark:hover:text-primary-text-color-dark bg-inverse-on-surface dark:bg-inverse-on-surface-dark rounded-full py-[3px] px-3 flex items-center justify-center">
                                                    Hamısı</div>
                                                <div
                                                    class="iscilerTab text-[12px] font-medium cursor-pointer text-tertiary-text dark:text-tertiary-text-color-dark hover:text-messages dark:hover:text-primary-text-color-dark rounded-full py-[3px] px-3">
                                                    Aktiv balans</div>
                                                <div
                                                    class="iscilerTab text-[12px] font-medium cursor-pointer text-tertiary-text dark:text-tertiary-text-color-dark hover:text-messages dark:hover:text-primary-text-color-dark rounded-full py-[3px] px-3">
                                                    Passiv balans</div>
                                                <div
                                                    class="iscilerTab text-[12px] font-medium cursor-pointer text-tertiary-text dark:text-tertiary-text-color-dark hover:text-messages dark:hover:text-primary-text-color-dark rounded-full py-[3px] px-3">
                                                    Xərclənən</div>
                                                <div
                                                    class="iscilerTab text-[12px] font-medium cursor-pointer text-tertiary-text dark:text-tertiary-text-color-dark hover:text-messages dark:hover:text-primary-text-color-dark rounded-full py-[3px] px-3">
                                                    Kəşbek</div>
                                                <div
                                                    class="iscilerTab text-[12px] font-medium cursor-pointer text-tertiary-text dark:text-tertiary-text-color-dark hover:text-messages dark:hover:text-primary-text-color-dark rounded-full py-[3px] px-3">
                                                    Toplam</div>
                                            </div>
                                        </div>
                                    </div>
