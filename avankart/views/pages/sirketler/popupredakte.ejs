                                                            data-day="<%= day.name %>"
                                                            data-type="close"
                                                            maxlength="5"
                                                            <%= !isDayEnabled(day.name) ? 'disabled' : '' %>
                                                        />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    <% }); %>
