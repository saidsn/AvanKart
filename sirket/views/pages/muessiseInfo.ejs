<div class="w-full">
  <div class="w-full relative h-[200px]">
    <img src="<%= muessise.daxili_cover_image_path %>" alt="cover" class="w-full h-full" />
    <img class="profile-imageabsolute bottom-0 -translate-y-1/2 w-[100px] h-[100px] ml-[20px] rounded-full"
      src="<%= muessise.profile_image_path %>" alt="<%= muessise.muessise_name %> logo" />
  </div>

  <div class="flex w-[100%] mt-[62px]">
    <div class="w-[25%] px-[20px] border-r border-stroke pt-[12px] min-h-screen rounded-lg">
      <div class="bg-sidebar-bg py-[10px] px-[12px] mb-[8px] dark:bg-[#0C1418] rounded-sm">
        <p class="text-[12px] font-medium text-messages pb-1 dark:text-white">
          Dəyişiklik versiyaları
        </p>
        <span class="text-[11px] font-normal text-messages opacity-50 pr-[8px] dark:text-[#FFFFFF] opacity-50">
          Son düzəliş tarixi:
        </span>
        <span id="openPopupBtn"
          class="text-[11px] font-normal text-tertiary-text hover:text-messages dark:text-tertiary-text-color-dark dark:hover:text-primary-text-color-dark cursor-pointer group">
          <% const date=new Date(muessise.updatedAt); const gun=String(date.getDate()).padStart(2, '0' ); const
            ay=String(date.getMonth() + 1).padStart(2, '0' ); const il=date.getFullYear(); const
            formattedDate=`${gun}.${ay}.${il}`; %>
            <span
              class="group-hover:border-b group-hover:border-messages dark:group-hover:border-b dark:group-hover:border-primary-text-color-dark">
              <%= formattedDate %>
            </span>
            <div
              class="icon stratis-chevron-up rotate-180 text-[10px] mb-[1px] w-2.5 h-2.5 ms-3 dark:text-primary-text-color-dark">
            </div>
        </span>
        <%- include('../partials/muessise/editHistory.ejs') %>
      </div>
      <div class="bg-sidebar-bg py-[5.5px] px-[12px] dark:bg-[#0C1418] rounded-sm">
        <span class="text-[11px] font-normal text-messages opacity-50 pr-[4px] dark:text-[#FFFFFF] opacity-65">Üzv
          olduğu tarix:</span><span class="text-[12px] font-medium text-messages dark:text-[#FFFFFF]">
          <%= new Date(muessise.createdAt).toLocaleString('tr-TR', { day: '2-digit' , month: '2-digit' , year: 'numeric'
            , hour: '2-digit' , minute: '2-digit' }) %>
        </span>
      </div>
      <div class="mt-[16px]">
        <div class="flex justify-between items-center">
          <p class="muessise-name text-[22px] font-semibold text-messages dark:text-[#FFFFFF]">
            <%= muessise.muessise_name %>
          </p>
          <button onclick="openMainEditPopup()"
            class="border-[1px] border-surface-variant w-[107px] h-[25px] flex gap-[10px] justify-center items-center rounded-[100px] dark:text-[#FFFFFF] hover:cursor-pointer hover:text-focus text-[12px] font-medium text-messages dark:hover:text-focus">
            <span class="icon iconex-edit-1-1 !w-[10px] !h-[10px] text-messages dark:text-[#FFFFFF]"></span>Redaktə
            et
          </button>
        </div>
      </div>
      <div class="mt-1">
        <span class="py-[2.5px] flex items-center gap-[4px]">
          <span class="font-normal text-[12px] text-messages dark:text-[#FFFFFF] opacity-65 mr-1">ID:
          </span>
          <span class="muessise-id font-normal text-[12px] text-messages dark:text-[#FFFFFF] opacity-65">
            <%= muessise.sirket_id %>
          </span>
          <span onclick="copyToClipboard('<%= muessise.sirket_id %>')"
            class="icon iconex-copy-1 w-[12px] h-[15px] ml-[6px] dark:text-[#FFFFFF] cursor-pointer"></span>
        </span>
      </div>
      <p
        class="muessise-description pt-2 font-normal text-[12px] text-messages opacity-65 pb-[20px] border-b-[0.5px] border-[#0000001A] dark:text-[#FFFFFF] opaicity-65">
        <%= muessise.description %>
      </p>
      <div class="flex gap-4 flex-col pb-[20px] border-b-[0.5px] pt-4 border-[#0000001A]">
        <div class="flex gap-[11px] items-center text-messages">
          <span class="icon iconex-bookmark-1 text-xl dark:text-[#FFFFFF]"></span>
          <span class="muessise-category opacity-65 text-[13px] font-normal dark:text-[#FFFFFF]">
            <%= muessise.activity_type %>
          </span>
        </div>
        <div class="flex gap-[11px] items-center text-messages">
          <span class="icon iconex-location-1 text-xl dark:text-[#FFFFFF]"></span>
          <span class="muessise-address opacity-65 text-[13px] font-normal dark:text-[#FFFFFF]">
            <%= muessise.address %>
          </span>
        </div>
        <% if(muessise.phone.length> 0 ){ %>
          <div class="muessise-phones flex flex-col gap-[11px]">
            <% muessise.phone.forEach(function(p) { %>
              <div class="flex gap-[11px] items-center text-messages">
                <span class="icon iconex-calling-1 text-xl dark:text-[#FFFFFF]"></span>
                <span class="opacity-65 text-[13px] font-normal dark:text-[#FFFFFF]">
                  <%= p.prefix %>
                    <%= p.number %>
                </span>
              </div>
              <% }) %>
          </div>
          <% } %>
            <% muessise.email.forEach(function(mail) { %>
              <div class="flex gap-[11px] items-center text-messages">
                <span class="icon iconex-case-1 text-xl dark:text-[#FFFFFF]"></span>
                <span class="opacity-65 text-[13px] font-normal dark:text-[#FFFFFF]">
                  <%= mail %>
                </span>
              </div>
              <% }) %>
      </div>
      <div class="pt-4 pb-[20px] border-b-[0.5px] border-[#0000001A]">
        <p class="text-[11px] font-medium opacity-50 text-messages pb-2 dark:text-[#FFFFFF] opacity-50">
          Kartlar
        </p>
        <div class="muessise-cards flex flex-wrap gap-3">
          <% muessise.cards.forEach(function(card) { %>
            <div class="w-[84px] h-[27px] flex gap-1 items-center justify-center rounded-full"
              style="background-color: <%= card.background_color %>">
              <img class="w-[16px] h-[16px]" src="https://api.avankart.com/icons/<%= card.icon %>.svg" alt="<%= card.name %>" />
              <span class="font-normal text-[12px] text-messages">
                <%= card.name %>
              </span>
            </div>
            <% }) %>
        </div>
      </div>
      <div class="pt-4 pb-5 border-b-[0.5px] border-[#0000001A]">
        <p class="text-[11px] font-medium opacity-50 text-messages pb-2 dark:text-[#FFFFFF] opacity-50">
          İş günləri
        </p>
        <div class="flex gap-1 flex-wrap">
          <div class="flex gap-2 flex-wrap">
            <% Object.entries(muessise.schedule).forEach(([day, info])=> {
              const shortDay = day.slice(0, 2).toUpperCase();
              %>
              <div class="relative group">
                <div
                  class="w-[34px] h-[34px] rounded-full flex items-center justify-center text-xs <%= info.isOpen ? 'bg-focus text-on-primary' : 'bg-stroke text-tertiary-text' %>">
                  <%= shortDay %>
                </div>
                <div
                  class="<%= info.isOpen ? '' : 'hidden' %> absolute bottom-full mb-2 left-1/2 -translate-x-1/2 w-max px-2 py-1 text-xs text-on-primary bg-messages rounded opacity-0 group-hover:opacity-100 transition-opacity z-10">
                  <%= info.isOpen ? `${info.open} - ${info.close}` : 'Bağlıdır' %>
                    <div
                      class="absolute top-full left-1/2 -translate-x-1/2 w-0 h-0 border-l-8 border-r-8 border-t-[8px] border-t-messages border-transparent z-15">
                    </div>
                </div>
              </div>
              <% }) %>
          </div>
        </div>
      </div>
      <div class="pt-4 pb-5 border-b-[0.5px] border-[#0000001A]">
        <p class="text-[11px] font-medium opacity-50 text-messages pb-2 dark:text-[#FFFFFF] opacity-50">
          Sosial media
        </p>
        <div class="flex flex-col muessise-social">
          <div
            class="p-3 flex gap-2 border-b-[0.5px] border-[#0000001A] hover:bg-table-hover dark:hover:bg-[#242C30] cursor-pointer">
            <img src="/images/muessise melumatlari images/devicon_linkedin.svg" alt="linkedin" />
            <span class="text-[13px] font-medium text-messages dark:text-[#FFFFFF] opacity-65">Linkedin</span>
          </div>
          <div
            class="p-3 flex gap-2 border-b-[0.5px] border-[#0000001A] hover:bg-table-hover dark:hover:bg-[#242C30] cursor-pointer">
            <img src="/images/muessise melumatlari images/skill-icons_instagram.svg" alt="linkedin" />
            <span class="text-[13px] font-medium text-messages dark:text-[#FFFFFF] opacity-65">İnstagram</span>
          </div>
          <div
            class="p-3 flex gap-2 border-b-[0.5px] border-[#0000001A] hover:bg-table-hover dark:hover:bg-[#242C30] cursor-pointer">
            <img src="/images/muessise melumatlari images/devicon_facebook.svg" alt="linkedin" />
            <span class="text-[13px] font-medium text-messages dark:text-[#FFFFFF] opacity-65">Facebook</span>
          </div>
        </div>
      </div>
      <div class="pt-4 pb-5 border-b-[0.5px] border-[#0000001A] mb-[17px]">
        <p class="text-[11px] font-medium opacity-50 text-messages pb-2 dark:text-[#FFFFFF]">
          Web sayt
        </p>
        <div class="muessise-websites py-3flex flex-col gap-2">
          <% muessise.website.forEach(function(site) { %>
            <a href="<%= site %>" target="_blank" rel="noopener noreferrer"
              class="flex gap-2 items-center hover:bg-table-hover dark:hover:bg-[#242C30] cursor-pointer py-2 px-2 rounded">
              <div class="icon iconex-link-1 text-xl dark:text-[#FFFFFF]"></div>
              <span class="text-[13px] font-medium text-messages dark:text-[#FFFFFF] opacity-65">
                <%= site.replace(/^https?:\/\//, '' ) %>
              </span>
            </a>
            <% }) %>
        </div>
      </div>
    </div>
    <div class="max-w-[75%] px-2 w-[100%]">
      <div class="flex flex-col">
        <div class="py-3 flex flex-col gap-4 border-stroke mb-3">
          <div class="border-b border-stroke inline-block">
            <ul class="inline-flex flex-wrap gap-5 -mb-px text-[13px] font-medium text-center text-tertiary-text">
              <li>
                <a href="#" id="main-tab-users"
                  class="main-tab-button inline-flex items-center justify-center py-2 text-messages border-b-2 border-messages rounded-t-lg hover:text-messages dark:text-[#FFFFFF] hover:border-b-2 hover:border-messages group dark:border-b-[#FFFFFF]"
                  data-content="users-content">
                  İstifadəçilər
                </a>
              </li>
              <li>
                <a href="#" id="main-tab-rekvizitler"
                  class="main-tab-button inline-flex items-center justify-center py-2 text-messages border-b-2 border-messages rounded-t-lg hover:text-messages dark:text-[#FFFFFF] hover:border-b-2 hover:border-messages group dark:border-b-[#FFFFFF]"
                  data-content="rekvizitler-content">
                  Rekvizitlər
                </a>
              </li>
              <li>
                <a href="#" id="main-tab-muqavileler"
                  class="main-tab-button inline-flex items-center justify-center py-2 text-messages border-b-2 border-messages rounded-t-lg hover:text-messages dark:text-[#FFFFFF] hover:border-b-2 hover:border-messages group dark:border-b-[#FFFFFF]"
                  data-content="muqavileler-content">
                  Müqavilələr
                </a>
              </li>
            </ul>
          </div>
        </div>

        <div id="main-content-area" class="flex flex-col">
          <div id="users-content" class="main-content-block" style="display: block">
            <div id="original-controls-container"
              class="flex items-center justify-between gap-4 mb-3 users-controls-and-tables">
              <div class="flex items-center">
                <div
                  class="inline-flex gap-1 items-center border border-surface-variant rounded-full p-1 dark:bg-[#161E22] dark:border-[#40484C]">
                  <button id="tab-users"
                    class="notification-type bg-inverse-on-surface text-messages active rounded-full py-[3px] px-3 text-[12px] font-medium dark:bg-[#2E3135] dark:text-[#FFFFFF]">
                    İstifadəçilər
                  </button>
                  <button id="tab-permissions"
                    class="notification-type text-tertiary-text cursor-pointer rounded-full py-[3px] px-3 text-[12px] font-medium dark:text-[#FFFFFF80]">
                    Səlahiyyət qrupları
                  </button>
                  <button id="tab-roles"
                    class="notification-type text-tertiary-text cursor-pointer rounded-full py-[3px] px-3 text-[12px] font-medium dark:text-[#FFFFFF80]">
                    Vəzifələr
                  </button>
                </div>
              </div>
              <div class="flex items-center gap-2">
                <div
                  class="relative flex items-center max-w-[237px] border border-surface-variant dark:border-surface-variant-dark rounded-full overflow-hidden focus-within:border-focus focus-within:shadow-[0px_0px_0px_2.5px_var(--color-button-disabled)] focus-within:ring-0 focus-within:outline-none active:border-focus active:shadow-[0px_0px_0px_2.5px_var(--color-button-disabled)] transition-all ease-out duration-300 hover:bg-input-hover dark:hover:bg-input-hover-dark">
                  <input id="customSearch" type="text" placeholder="Search..."
                    class="w-full h-full bg-transparent border-none focus:outline-none outline-none ring-0 focus:ring-0 text-[13px] text-messages dark:text-primary-text-color-dark placeholder:text-on-surface-variant-dark dark:placeholder:text-[#636B6F]" />
                  <div class="absolute right-3">
                    <div class="icon stratis-search-02 dark:text-primary-text-color-dark"></div>
                  </div>
                </div>
                <div id="refreshPage"
                  class="max-w-[155px] cursor-pointer border border-surface-variant rounded-full flex items-center justify-center gap-2 text-[13px] text-messages hover:text-focus font-medium py-[6.5px] px-[18px] dark:border-[#40484C] dark:bg-[#161E22] dark:text-[#FFFFFF] dark:hover:text-focus">
                  <span>Səhifəni yenilə</span>
                  <span class="icon stratis-arrow-refresh-04 text-messages dark:text-[#FFFFFF]"></span>
                </div>
                <button id="add-button"
                  class="add-button cursor-pointer bg-primary hover:bg-focus text-on-primary border border-surface-variant rounded-full flex items-center justify-center gap-2 text-[13px] font-medium py-[6.5px] px-[18px] dark:border-none">
                  <div class="icon stratis-plus-02 mt-[2.5px]"></div>
                  <span class="add-button-text">Hesab əlavə et</span>
                </button>
              </div>
            </div>

            <div id="selection-controls"
              class="border-b border-stroke dark:border-[#40484C] pb-3 flex items-center justify-between gap-4 mb-3 users-controls-and-tables"
              style="visibility: hidden; opacity: 0">
              <div class="flex items-center gap-6">
                <div class="flex items-center gap-4 border-r-[.5px] border-stroke dark:border-[#40484C] pr-2">
                  <p class="font-semibold dark:text-primary-text-color-dark">
                    Seçilən istifadəçi sayı:
                  </p>
                  <p id="selected-count"
                    class="flex items-center justify-center w-6 h-6 bg-gray-50 dark:bg-menu-dark dark:text-primary-text-color-dark border-[0.5px] dark:border-[#40484C]">
                    0
                  </p>
                </div>
                <button id="select-all-btn"
                  class="cursor-pointer dark:text-primary-text-color-dark text-sm font-semibold px-2 py-1 rounded-full hover:bg-[#F6D9FF] dark:hover:bg-[#5B396D4D]">
                  Hamısını seç
                </button>
                <button id="clear-selection-btn"
                  class="cursor-pointer dark:text-primary-text-color-dark text-sm font-semibold hover:bg-[#F6D9FF] dark:hover:bg-[#5B396D4D] px-2 py-1 rounded-full">
                  Seçimləri təmizlə
                </button>
                <button id="delete-selected-btn" onclick="showDeleteItemPopup()"
                  class="cursor-pointer text-sm flex items-center gap-1 font-semibold text-error hover:bg-error-hover px-2 py-1 rounded-full">
                  <div class="icon stratis-trash-01 text-error"></div>
                  Seçilənləri sil
                </button>
              </div>
            </div>

            <div id="overlay" class="hidden fixed inset-0 w-full h-full bg-[#0000002A] z-100"></div>

            <%- include('../partials/muessise/hesabAdd.ejs') %> <%- include('../partials/muessise/editUser.ejs') %>

                <!-- Yeni Qrup Popup -->
