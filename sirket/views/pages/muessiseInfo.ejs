                    <img src="/images/Sorgular Pages Images/Close.svg" alt="Close" onclick="closeYeniQrupPopup()"
                      class="cursor-pointer block dark:hidden" />
                    <img src="/images/avankart-partner-pages-images/Close-dark.svg" alt="Close"
                      onclick="closeYeniQrupPopup()" class="cursor-pointer hidden dark:block" />
                  </div>
                  <div class="mt-5">
                    <div class="text-[12px] font-medium opacity-65 mb-1.5 dark:text-[#FFFFFFA6]">
                      Qrup adı
                    </div>
                    <input type="text" placeholder="Qrup adını daxil edin"
                      class="dark:text-primary-text-color-dark placeholder:text-on-surface-variant-dark dark:placeholder:text-outline-dark cursor-pointer w-full h-[34px] font-poppins font-normal text-[12px] leading-[160%] border-[1px] border-surface-variant rounded-full pl-[12px] hover:bg-input-hover focus:border-focus focus:shadow-[0px_0px_0px_2.5px_var(--color-button-disabled)] focus:ring-0 focus:outline-none active:border-focus active:shadow-[0px_0px_0px_2.5px_var(--color-button-disabled)] transition-all ease-out duration-300 dark:bg-table-hover-dark dark:border-on-surface-variant" />
                  </div>
                  <div class="flex items-center justify-end gap-3 mt-[30px]">
                    <button type="button"
                      class="bg-surface rounded-full text-[13px] px-[18px] py-[6.5px] cursor-pointer hover:text-messages dark:bg-[#36393E] dark:text-[#BFC8CC]"
                      onclick="closeYeniQrupPopup()">
                      Ləğv et
                    </button>
                    <button type="button" onclick="clickBasMuhasiblerPop()"
                      class="bg-primary rounded-full text-white text-[13px] px-[18px] py-[6.5px] cursor-pointer dark:hover:bg-[#88649A] transition">
                      İrəli
                    </button>
                  </div>
                </div>

                <%- include('../partials/muessise/basMuhasibler.ejs') %> <%-
                    include('../partials/muessise/butunIstifadecilerPop.ejs') %>

                    <!-- Yeni Vəzifə Popup -->
